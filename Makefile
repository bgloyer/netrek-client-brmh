all:: randomize

randomize: randomize.o $(RAND_OBJS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) randomize.o $(RAND_OBJS)  $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

cleandir::
	$(RM) randomize

all:: $(TARGET)

$(TARGET):$(P) $(OBJS)  ./randomize
	$(RM) $@
	$(CC) -o $@ `./randomize $(OBJS)` $(LDOPTIONS)  $(LDLIBS)  $(EXTRA_LOA D_FLAGS)

cleandir::
	$(RM) $(TARGET)

install:: $(TARGET)
	@if [ -d $(DESTDIR)$(NETREK_BIN) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(NETREK_BIN) ]; then \
			(set -x; rm -f $(DESTDIR)$(NETREK_BIN)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(NETREK_BIN)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  $(TARGET) $(DESTDIR)$(NETREK_BIN)/$(TARGET)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

mkkey: mkkey.o $(RAND_OBJS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) mkkey.o $(RAND_OBJS)  $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

cleandir::
	$(RM) mkkey

netrektest: BIG.o
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) BIG.o  $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

cleandir::
	$(RM) netrektest

BIG.c: protos.h
	$(RM) allincludes.h
	grep -h "^#include" $(SRCS) > allincludes.h
	cat allincludes.h protos.h $(SRCS) > BIG
	mv BIG BIG.c

protos.h:
	$(RM) protos.h
	cproto -e -f3 -m -D__STDC__ -D_NO_DEPEND -DCPROTO $(EXTRA_DEFINES) $(ALLINCLUDES) $(SRCS) > protos.h

World ::
	$(MAKE) $(MFLAGS) depend
	$(MAKE) $(MFLAGS) all

netrek.dec:
	$(RM) netrek
	purify $(CC) $(CFLAGS) -o netrek `./randomize $(SRCS)` $(LDOPTIONS) 		$(LDLIBS) $(EXTRA_LOAD_FLAGS)

xdebug_packets: xdebug_packets.o
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) xdebug_packets.o $(XAWLIB) $(XMULIBONLY) $(XTOOLLIB) $(XPMLIB) $(XLIB) $(LDLIBS) -lm $(EXTRA_LOAD_FLAGS)

cleandir::
	$(RM) xdebug_packets

protos::
	$(RM) protos.h
	cproto -e -f3 -m -D__STDC__ -D_NO_DEPEND -DCPROTO $(EXTRA_DEFINES) $(ALLINCLUDES)  $(SRCS) > protos.h

proto-%:
	cproto -p -m -s -f3 -D__STDC__ -D_NO_DEPEND -DCPROTO $(EXTRA_DEFINES) $(ALLINCLUDES) $* | grep static

